if $noOutPutComAlone!=1
    @import "../importCom"
$selector-pagenum='.pagenum'
// 分页条
// 页码按钮使用的是按钮组组件,样式都是.btn的,还有input-text,算是一个组合组件,所以样式很少.

{$selector-pagenum}
    // 主题色变量
    //--t-pagenum-active-color:$white['1']// 当前页字色
    --t-pagenum-active-bg-color:$gray['3']// 当前页背景色
    --t-pagenum-hover:$gray['1']// hover背景色
    // 外框
    user-select:none
    // 页码按钮框
    &-btns
        min-width:50%
    // 当前选中页 
    &-active,&-active:hover
        background-color: var(--t-pagenum-active-bg-color)

    // 总页码,跳转区域 框   
    &-skip
        margin-left: 1em
    &-input
        display:inline-block
        width: 3.6em
        margin: 0 4px
        text-align:center
        
    &-input:focus
        outline: none

if $themeTag is defined
    {$themePre}-dark {$selector-pagenum}
        --t-pagenum-active-bg-color:$gray['5']// 当前页背景色
        --t-pagenum-hover:$gray['4']// hover背景色
    // 彩色主题
    for k,v in $colors-dict
        {$selector-pagenum}.{k}
            --t-pagenum-active-bg-color:v['5']// 当前页背景色
        {$selector-pagenum}.{k} {$selector-pagenum}-active
            @extend $ext-color-white